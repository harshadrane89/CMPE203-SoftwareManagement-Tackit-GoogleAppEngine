<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>TackIT</display-name>
  <servlet>
    <servlet-name>LoginVerify</servlet-name>
    <servlet-class>com.tackit.servlet.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginVerify</servlet-name>
    <url-pattern>/LoginVerify</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>UManager</servlet-name>
    <servlet-class>com.tackit.servlet.UserManager</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UManager</servlet-name>
    <url-pattern>/UManager</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>SBoard</servlet-name>
    <servlet-class>com.tackit.servlet.SBoardProcessor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SBoard</servlet-name>
    <url-pattern>/SBoard</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CreateUser</servlet-name>
    <servlet-class>com.tackit.servlet.Signup</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateUser</servlet-name>
    <url-pattern>/CreateUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Board</servlet-name>
    <servlet-class>com.tackit.servlet.BoardProcessor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Board</servlet-name>
    <url-pattern>/Board</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BoardCreator</servlet-name>
    <servlet-class>com.tackit.servlet.CreateBoard</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardCreator</servlet-name>
    <url-pattern>/BoardCreator</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PinInfo</servlet-name>
    <servlet-class>com.tackit.servlet.PinProcessor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PinInfo</servlet-name>
    <url-pattern>/PinInfo</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>Login.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>